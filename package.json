{
  "name": "dpr-backend",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "typecheck": "bun scripts/run typecheck",
    "typeconfig": "bun scripts/run typeconfig",
    "seed": "bun scripts/run seed",
    "dev": "bun scripts/run dev",
    "test": "bun scripts/run test",
    "test:unit": "bun scripts/run test:unit",
    "coverage": "bun scripts/run coverage",
    "test:e2e": "bun scripts/run test:e2e",
    "build": "bun scripts/run build",
    "start": "bun scripts/run start",
    "serve": "bun scripts/run serve",
    "clean": "bun scripts/run clean",
    "docker": "bun scripts/run docker",
    "docker:run": "bun scripts/run docker:run",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "workspace": "bun scripts/workspace"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*",
      "handlers/*",
      "converters/*",
      "playground/*"
    ],
    "catalog": {
      "@types/bun": "latest",
      "typescript": "^5",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "@types/react": "^19",
      "@types/react-dom": "^19",
      "@dpr/dev-config": "workspace:*",
      "@dpr/config": "workspace:*",
      "@dpr/ui": "workspace:*",
      "@dpr/api": "workspace:*",
      "@dpr/libs": "workspace:*",
      "@dpr/converter-bops": "workspace:*",
      "@dpr/application-generator": "workspace:*",
      "@dpr/handler-local": "workspace:*",
      "@dpr/handler-bops": "workspace:*",
      "@elysiajs/eden": "latest",
      "@elysiajs/openapi": "latest",
      "elysia": "latest",
      "@sinclair/typebox": "^0.34.41",
      "http-status-codes": "^2.3.0",
      "@dpr/odp-schemas": "workspace:*",
      "dayjs": "^1.11.18",
      "@faker-js/faker": "^8.4.1",
      "vite": "^7.1.7"
    },
    "catalogs": {
      "build": {},
      "testing": {}
    }
  }
}
